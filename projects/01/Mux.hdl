/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    And(a=sel,b=b,out=tempB);
    Not(in=sel,out=notSel);
    And(a=notSel,b=a,out=tempA);
    Or(a=tempA,b=tempB,out=out);
}